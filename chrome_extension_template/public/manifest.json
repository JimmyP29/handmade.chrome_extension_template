{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Chrome Extension Template",
  "action": {
    "default_popup": "index.html"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["scripting", "tabs", "sidePanel", "commands"],
  "host_permissions": ["http://*/*", "https://*/*"]
}
